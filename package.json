{
  "name": "serverless-monitor",
  "displayName": "serverless-monitor",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:fnPerStage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fnPerStage.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "fnPerStage.addEntry",
        "title": "Add"
      },
      {
        "command": "fnPerStage.editEntry",
        "title": "Edit",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "fnPerStage.editEntry2",
        "title": "Edit",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "fnPerStage.deleteEntry",
        "title": "Delete"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "serverless-monitor",
          "title": "Serverless Monitor",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "serverless-monitor": [
        {
          "id": "fnHandlerList",
          "name": "Function Handlers"
        },
        {
          "id": "fnPerStage",
          "name": "Logs per stage"
        },
        {
          "id": "lastFailed",
          "name": "Last failed requests"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "ftpExplorer.revealResource"
        }
      ],
      "view/title": [
        {
          "command": "fnPerStage.refreshEntry",
          "when": "view == fnPerStage",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "fnPerStage.editEntry",
          "when": "view == fnPerStage && viewItem == function",
          "group": "inline"
        },
        {
          "command": "fnPerStage.editEntry2",
          "when": "view == fnPerStage && viewItem == function",
          "group": "inline"
        },
        {
          "command": "fnPerStage.deleteEntry",
          "when": "view == fnPerStage && viewItem == function"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "@types/yaml": "^1.0.2",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "aws-sdk": "^2.541.0",
    "moment": "^2.24.0",
    "yaml": "^1.7.0"
  }
}
